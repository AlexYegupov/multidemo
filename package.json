{
  "name": "multidemo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "prettify": "prettier --check src/**/*.{js,jsx}",
    "prettify:fix": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "build-docs": "rm -rf ./docs/_sidebar.md && yarn build-docs:remark && yarn build-docs:docsify",
    "build-docs:remark": "yarn remark -f ./docs -o ./docs",
    "build-docs:docsify": "./bin/build-docsify.sh",
    "preview-docs": "(cd docs && python3 -m http.server $PORT||3000)"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@wttsolutions/eslint-config": "^0.0.54",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "hast-util-is-element": "^2.1.2",
    "remark": "^14.0.2",
    "remark-cli": "^11.0.0",
    "remark-gfm": "^3.0.1",
    "remark-lint": "^9.1.1",
    "remark-lint-no-file-name-irregular-characters": "^2.1.1",
    "remark-lint-no-undefined-references": "^4.2.0",
    "remark-validate-links": "^12.1.0",
    "unist-util-visit": "^4.1.1",
    "concurrently": "^7.6.0",
    "docsify-cli": "^4.4.4",
    "remark-autolink-references": "^2.0.0",
    "remark-lint-no-dead-urls": "^1.1.0"
  }
}
